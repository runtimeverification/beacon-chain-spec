	Keywords = {BPEL specification;NFR approach;UML diagram;business process execution language;complex computing system;elevator control system;nonfunctional requirement;unified modeling language;Unified Modeling Language;Web services;formal specification;},
	Month = {5-8},
	Pages = {153 -158},
	Title = {Evaluating consistency between {BPEL} specifications and functional requirements of complex computing systems using the {NFR} approach},
	Year = {2010},
