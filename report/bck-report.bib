	Note = {Selected Papers from the Ninth International Workshop on Rewriting Logic and its Applications (WRLA 2012)},
	Pages = {128 - 192},
	Title = {Sound and complete timed {CTL} model checking of timed {Kripke} structures and real-time rewrite theories},
