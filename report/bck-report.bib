	Isbn = {978-3-642-40392-7},
	Pages = {97--113},
	Publisher = {Springer Berlin Heidelberg},
	Title = {Secure and Lightweight Distance-Bounding},
	Year = {2013},
	Bdsk-File-1 = {}}

