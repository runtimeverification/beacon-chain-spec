	Pages = {69-85},
	Publisher = {Springer Berlin / Heidelberg},
	Series = {Lecture Notes in Computer Science},
	Title = {A {Church-Rosser} Checker Tool for Conditional Order-Sorted Equational {Maude} Specifications},
	Volume = {6381},
