	Keywords = {Internet-scale service-oriented platform;Web service;digital ecosystem;service-oriented architecture;Web services;},
	Month = Feb.,
	Pages = {67 -70},
	Title = {Towards Improving the Availability of Service Compositions},
	Year = {2007},
